{
    "name": "grammy-pug",
    "version": "1.0.3",
    "scripts": {
        "dev": "npx tsx watch src/dev.ts",
        "start": "npx tsx src/dev.ts",
        "build": "npx tsup src/index.ts",
        "prepublish": "yarn build"
    },
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "import": {
                "types": "./dist/index.d.mts",
                "default": "./dist/index.mjs"
            },
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "files": [
        "dist/"
    ],
    "keywords": [
        "typescript",
        "grammy",
        "plugin",
        "grammy-plugin",
        "pug"
    ],
    "author": "Rul",
    "license": "MIT",
    "description": "i18n with pug for grammy",
    "devDependencies": {
        "@types/node": "^25.0.3",
        "@types/pug": "^2.0.10",
        "tsup": "^8.5.1",
        "typescript": "^5.9.3"
    },
    "dependencies": {
        "grammy": "^1.38.4",
        "pug": "^3.0.3"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Rul991/grammy-pug.git"
    },
    "publishConfig": {
        "access": "public"
    }
}
